    "@types/xml2js": "^0.4.11",
    "@types/yargs": "^17.0.10",
    "archiver": "^5.3.1",
    "nexe": "^4.0.0-rc.1",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "nodemon": "^2.0.16",
    "open": "^8.4.0",
    "request": "^2.88.2",
    "soundpad": "github:mcpe-pc/soundpad.js",
